Proceso CalcularHipotenusa
	Definir caopuesto,caadyasente,hipotenusa Como Real
	Escribir "digite el cateto opuesto";
	Leer caaopuesto;
	Escribir "digite el cateto adyasente";
	Leer caadyasente;
	hipotenusa<-(caaopuesto^2 + caadyasente^2)^(1/2)
	
	Escribir "la hipotenusa es:" , hipotenusa 
	
	
FinProceso
