Proceso DosNumerosEnterosCalculadora
	Definir d1,d2,suma,resta,multiplicacion,division como entero 
	Escribir "digite el 1er numero"; 
	leer d1;
	Escribir "digite 2do numero";
	leer d2;
	
	suma<-d1+d2	
	resta<-d1-d2
	multiplicacion<-d1*d2
	division<-d1/d2
	
	Escribir "la suma es",suma;
	Escribir "la resta es ",resta;
	Escribir "la multiolicacion es ",multiplicacion;
	Escribir "la division es",division;
	
	
	
FinProceso
